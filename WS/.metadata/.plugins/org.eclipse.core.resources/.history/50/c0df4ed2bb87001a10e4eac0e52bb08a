
public class KarakterlerSayidir {

	public static void main(String[] args) {
		char ch = 'C';
		int i = ch;
		System.out.println(ch);
		System.out.println(i);
		ch++;
		System.out.println(ch);
		
		for (char c = 0; c < 64_000; c++) {
			System.out.print(c + " ");
			if ((c + 1) % 70 == 0)
				System.out.println();
		}

	}

}
